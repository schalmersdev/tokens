{
  "animation": {
    "$description": "Random tokens",
    "basics": {
      "Duration-100": {
        "$type": "duration",
        "$value": "100ms",
        "$description": "Average durations of an animation"
      },
      "small": {
        "$type": "cubicBezier",
        "$value": [
          0,
          0,
          0.5,
          1
        ],
        "$description": "Basic animation coordinates"
      }
    }
  },
  "base": {
    "$description": "All the base color tokens",
    "$type": "color",
    "backgrounds": {
      "dark-primary": {
        "$type": "color",
        "$value": "{base.palette.primary30}"
      },
      "light-primary": {
        "$type": "color",
        "$value": "{base.palette.primary90}"
      }
    },
    "palette": {
      "primary30": {
        "$type": "color",
        "$value": "#4F378B"
      },
      "primary50": {
        "$type": "color",
        "$value": "#826BBB"
      },
      "primary70": {
        "$type": "color",
        "$value": "#B6A1E9"
      },
      "primary90": {
        "$type": "color",
        "$value": "#EADDFF"
      }
    }
  },
  "sizes": {
    "$type": "dimension",
    "base spacing": {
      "$type": "dimension",
      "$value": "0.25rem"
    },
    "digital": {
      "devices": {
        "mobile": {
          "$type": "dimension",
          "$value": "{sizes.base spacing}"
        },
        "tablet": {
          "$type": "dimension",
          "$value": "80px"
        }
      },
      "web": {
        "$type": "dimension",
        "$value": "4rem"
      }
    }
  },
  "typography": {
    "font-weight-default": {
      "$type": "fontWeight",
      "$value": 350
    },
    "primary font": {
      "$type": "fontFamily",
      "$value": "Comic Sans MS"
    }
  }
}